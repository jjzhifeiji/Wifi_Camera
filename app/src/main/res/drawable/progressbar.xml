<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient
                android:angle="0.0"
                android:endColor="#ff78aef5"
                android:startColor="#ff78aef5" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient
                    android:angle="0.0"
                    android:endColor="#ff78aef5"
                    android:startColor="#ff78aef5" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient
                    android:angle="0.0"
                    android:endColor="#ff01ff01"
                    android:startColor="#ff93ff01" />
            </shape>
        </clip>
    </item>
</layer-list>